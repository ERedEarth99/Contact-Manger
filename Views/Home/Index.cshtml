@model IEnumerable<Contact>

@{
    ViewData["Title"] = "Home";
}

<h2>Contacts</h2>

<a class="btn btn-primary"></a>
<table class="table table-striped">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Phone</th>
            <th>Email</th>
            <th>Category</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var contact in Model)
        {
            <tr>
                <td>
                    <a>Details@contact.FirstName</a>
                </td>
                <td>
                    <a>asp-action="Details" </a>
                </td>
                <td>@contact?.Phone</td>
                <td>@contact?.Email</td>
                <td>@contact?.Category?.Name</td>
            </tr>
        }
    </tbody>
</table>